{
  "name": "xene",
  "version": "0.2.25",
  "description": "Conversational bot framework",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "rm -rf ./js && tsc",
    "lint": "tslint --project tsconfig.json",
    "test": "nyc ava",
    "dev": "./scripts/dev.sh"
  },
  "ava": {
    "files": [
      "js/test/**/*.js"
    ]
  },
  "nyc": {
    "include": [
      "js/**/*.js"
    ],
    "exclude": [
      "js/test/**/*.js"
    ],
    "reporter": [
      "html",
      "lcov"
    ],
    "check-coverage": true,
    "sourceMap": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dempfi/xene.git"
  },
  "author": "Ike Ku <dempfi@yahoo.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dempfi/xene/issues"
  },
  "homepage": "https://github.com/dempfi/xene#readme",
  "devDependencies": {
    "@types/lodash": "^4.14.50",
    "@types/node": "^6.0.47",
    "@types/request-promise-native": "^1.0.2",
    "@types/sinon": "^1.16.34",
    "@types/uuid": "^2.0.29",
    "ava": "^0.18.0",
    "nyc": "^10.1.2",
    "onchange": "^3.2.1",
    "sinon": "^1.17.7",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1"
  },
  "dependencies": {
    "@slack/client": "git+https://git@github.com/dempfi/node-slack-sdk.git#add-types",
    "lodash": "^4.17.4",
    "request-promise-native": "^1.0.3",
    "uuid": "^3.0.1"
  }
}
